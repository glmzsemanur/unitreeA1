<launch>


  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
  <remap from="/map" to="/elevation_to_occupancy/merged_global_map"/>
  <remap from="odom" to="/odometry/filtered" />
    <rosparam file="$(find elevation_mapping_demos)/config/param/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find elevation_mapping_demos)/config/param/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find elevation_mapping_demos)/config/param/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find elevation_mapping_demos)/config/param/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find elevation_mapping_demos)/config/param/base_local_planner_holonomic_params.yaml" command="load" />
    <rosparam file="$(find elevation_mapping_demos)/config/param/move_base_params.yaml" command="load" />
  </node>
</launch>
